
delta $"TABLE_1_LOC"
model Table1 = {
  id: string,
  name: string,
}

delta $"TABLE_2_LOC"
model Table2 = {
  my_id: string,
  tid: string,
  value: int,
}

model Table3 = 
  | Table2 
  | where value > 0
  | sort tid asc
  | with {
    x = 5,
  }

delta $"TABLE_4_OUTPUT"
model Table4 = Table1 join(id == tid) Table3