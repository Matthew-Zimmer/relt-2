
postgres $"TABLE_1_LOCATION"
model Table1 = {
  id: string,
  firstName: string,
  lastName: string,
}

postgres $"TABLE_2_LOCATION"
model Table2 = {
  xid: string,
	tid: string,
  value: int,
}

postgres $"TABLE_3_OUTPUT"
model Table3 = 
  | Table1
	| join(id == tid) Table2
  | with {
    fullName = firstName + " " + lastName,
  }
